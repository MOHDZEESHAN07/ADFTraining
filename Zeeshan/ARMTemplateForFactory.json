						"     isolationLevel: 'READ_UNCOMMITTED',",
						"     format: 'table') ~> customersource",
						"source(output(",
						"          CustomerID as integer,",
						"          Title as string,",
						"          FirstName as string,",
						"          MiddleName as string,",
						"          LastName as string,",
						"          Suffix as string,",
						"          CompanyName as string,",
