								"type": "LinkedServiceReference"
							}
						},
						{
							"dataset": {
								"referenceName": "moviesout",
								"type": "DatasetReference"
							},
							"name": "moviescountbyyear",
							"rejectedDataLinkedService": {
								"referenceName": "output",
								"type": "LinkedServiceReference"
							}
						}
					],
					"transformations": [
						{
							"name": "extractyeartitle"
						},
						{
							"name": "Ratingbyyear"
						}
					],
					"scriptLines": [
						"source(output(",
