{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Zeeshan"
		},
		"AzureBlobStorageinput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageinput'"
		},
		"AzureSqlDatabaseoutput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseoutput'"
		},
		"LS_cars_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_cars_source'"
		},
		"LS_csvtosql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_csvtosql'"
		},
		"blob_csvtodatset_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_csvtodatset'"
		},
		"output_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'output'"
		},
		"LS_cars_output_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_cars_output'"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "Zeeshan_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorageinput_connectionString": {
						"value": "[parameters('AzureBlobStorageinput_connectionString')]"
					},
					"AzureSqlDatabaseoutput_connectionString": {
						"value": "[parameters('AzureSqlDatabaseoutput_connectionString')]"
					},
					"LS_cars_source_connectionString": {
						"value": "[parameters('LS_cars_source_connectionString')]"
					},
					"LS_csvtosql_connectionString": {
						"value": "[parameters('LS_csvtosql_connectionString')]"
					},
					"blob_csvtodatset_connectionString": {
						"value": "[parameters('blob_csvtodatset_connectionString')]"
					},
					"output_connectionString": {
						"value": "[parameters('output_connectionString')]"
					},
					"LS_cars_output_connectionString": {
						"value": "[parameters('LS_cars_output_connectionString')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "Zeeshan_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'Zeeshan_ArmTemplate_0')]"
			]
		}
	]
}