{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "zeeshan12"
		},
		"AzureBlobStorageinput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorageinput'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDataLakeStorage2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage2'"
		},
		"AzureSqlDatabasecustomer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabasecustomer'"
		},
		"AzureSqlDatabaseoutput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabaseoutput'"
		},
		"AzureSqlLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlLinkedService'"
		},
		"LS_cars_output_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_cars_output'"
		},
		"LS_cars_source_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_cars_source'"
		},
		"LS_csvtosql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_csvtosql'"
		},
		"StorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'StorageLinkedService'"
		},
		"blob_csvtodatset_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'blob_csvtodatset'"
		},
		"customersource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'customersource'"
		},
		"moviesoutput_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'moviesoutput'"
		},
		"output_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'output'"
		},
		"sqldatabase_z_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldatabase_z'"
		},
		"sqldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'sqldb'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=zeeshan123.database.windows.net;Initial Catalog=master"
		},
		"zeeshan_b_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'zeeshan_b'"
		},
		"AzureDataLakeSQL_zeeshan_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zeeshan.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zeeshan.dfs.core.windows.net/"
		},
		"AzureDataLakeStorage2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zeeshan.dfs.core.windows.net/"
		},
		"zeeshan_blob_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://zeeshan.blob.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "zeeshan12_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureDataLakeStorage2_accountKey": {
						"value": "[parameters('AzureDataLakeStorage2_accountKey')]"
					},
					"AzureSqlLinkedService_connectionString": {
						"value": "[parameters('AzureSqlLinkedService_connectionString')]"
					},
					"StorageLinkedService_connectionString": {
						"value": "[parameters('StorageLinkedService_connectionString')]"
					},
					"moviesoutput_connectionString": {
						"value": "[parameters('moviesoutput_connectionString')]"
					},
					"AzureBlobStorageinput_connectionString": {
						"value": "[parameters('AzureBlobStorageinput_connectionString')]"
					},
					"AzureSqlDatabasecustomer_connectionString": {
						"value": "[parameters('AzureSqlDatabasecustomer_connectionString')]"
					},
					"AzureSqlDatabaseoutput_connectionString": {
						"value": "[parameters('AzureSqlDatabaseoutput_connectionString')]"
					},
					"blob_csvtodatset_connectionString": {
						"value": "[parameters('blob_csvtodatset_connectionString')]"
					},
					"customersource_connectionString": {
						"value": "[parameters('customersource_connectionString')]"
					},
					"sqldatabase_z_connectionString": {
						"value": "[parameters('sqldatabase_z_connectionString')]"
					},
					"sqldb_connectionString": {
						"value": "[parameters('sqldb_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage2_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage2_properties_typeProperties_url')]"
					},
					"AzureDataLakeSQL_zeeshan_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeSQL_zeeshan_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "zeeshan12_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"zeeshan_b_connectionString": {
						"value": "[parameters('zeeshan_b_connectionString')]"
					},
					"LS_cars_output_connectionString": {
						"value": "[parameters('LS_cars_output_connectionString')]"
					},
					"LS_cars_source_connectionString": {
						"value": "[parameters('LS_cars_source_connectionString')]"
					},
					"LS_csvtosql_connectionString": {
						"value": "[parameters('LS_csvtosql_connectionString')]"
					},
					"output_connectionString": {
						"value": "[parameters('output_connectionString')]"
					},
					"zeeshan_blob_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('zeeshan_blob_properties_typeProperties_serviceEndpoint')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'zeeshan12_ArmTemplate_0')]"
			]
		}
	]
}