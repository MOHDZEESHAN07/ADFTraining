				"typeProperties": {
					"connectionString": "[parameters('AzureSqlDatabasecustomer_connectionString')]"
				},
				"connectVia": {
					"referenceName": "integrationRuntime2",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
